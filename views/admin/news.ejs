<%- include('includes/head.ejs') %>
<title><%= pageTitle %></title>
</head>

<body class="admin">
    <%- include('includes/nav.ejs') %>
    <main>
        <div class="news">
            NEWS

            <ul class="news">
                <% news.forEach(function(item){ %>
                    <li>
                        <div class="title">
                            <%= item.title %>
                        </div>

                        <div class="body">
                            <%= item.body %>
                        </div>
                    </li>
                <% }) %>
            </ul>    
        </div>

        <a href="add-news">Add news</a>
    </main>        
</body>
</html>